{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Port=5432;Database=milk_db;Username=milk_user;Password=milk_password"
  },
  "AuditLog": {
    "EnableDatabaseWrite": true,
    "IncludedActions": "All"
  },
  "Ldap": {
    "Host": "ldap.example.com",
    "Port": 389,
    "UseSsl": false,
    "BindDn": "cn=admin,dc=example,dc=com",
    "BindPassword": "admin_password",
    "SearchBase": "ou=groups,dc=example,dc=com",
    "GroupFilter": "(objectClass=groupOfUniqueNames)"
  }
}
