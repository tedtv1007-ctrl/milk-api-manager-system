{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Ldap": {
    "Host": "ldap.example.com",
    "Port": 389,
    "UseSsl": false,
    "BindDn": "cn=admin,dc=example,dc=com",
    "BindPassword": "admin_password",
    "SearchBase": "ou=groups,dc=example,dc=com",
    "GroupFilter": "(objectClass=groupOfUniqueNames)"
  }
}
